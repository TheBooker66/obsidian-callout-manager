{
	"name": "@obsidian-callout-manager/plugin",
	"private": true,
	"version": "1.0.1",
	"description": "An Obsidian.md plugin that makes creating and configuring callouts easy.",
	"type": "module",
	"exports": {
		"import": "./dist/api-esm.mjs",
		"require": "./dist/api-cjs.js"
	},
	"imports": {
		"#api": "obsidian-callout-manager",
		"#ui/*": "./src/ui/*",
		"#ui-panes/*": "./src/panes/*",
		"#plugin": "./src/main",
		"#plugin-settings": "./src/settings"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"test": "jest",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && rollup -c rollup.config.mjs",
		"format": "prettier -w src"
	},
	"license": "MIT",
	"obsidianPlugin": {
		"name": "Callout Manager",
		"minAppVersion": "1.0.0",
		"description": "Easily create and customize callouts.",
		"isDesktopOnly": false
	},
	"dependencies": {
		"obsidian-extra": "^0.1.5",
		"obsidian": "1.1.1",
		"obsidian-callout-manager": "=1.1.0",
		"@obsidian-callout-manager/callout": "=1.1.0",
		"@obsidian-callout-manager/search": "=1.1.0",
		"@obsidian-callout-manager/sort": "=1.1.0",
		"@obsidian-callout-manager/color": "=1.1.0"
	},
	"devDependencies": {
		"@obsidian-callout-manager/build": "=0.0.0"
	}
}
